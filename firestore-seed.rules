// TEMPORARY Firebase Security Rules for Seeding
// ⚠️ WARNING: These rules allow ALL writes - use ONLY for seeding data
// After seeding, replace with proper security rules

rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow all reads and writes for seeding
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
